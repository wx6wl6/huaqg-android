<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:victory="http://schemas.victory.com/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<RelativeLayout
		android:id="@+id/section_progress"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:visibility="visible"
		>
		<RelativeLayout
			android:id="@+id/progLayout"
			android:layout_width="93dp"
			android:layout_height="90dp"
			android:background="@drawable/frame_box"
			android:layout_centerInParent="true"
			>
			<ProgressBar
				android:id="@+id/progressBar1"
				style="@android:style/Widget.Holo.ProgressBar.Large"
				android:layout_width="90dp"
				android:layout_height="90dp"
				android:layout_centerInParent="true"
				/>
			<TextView android:id="@+id/tvWaiting" style = "@style/textview_style_black14" android:visibility="gone"/>
		</RelativeLayout>
	</RelativeLayout>
</LinearLayout>